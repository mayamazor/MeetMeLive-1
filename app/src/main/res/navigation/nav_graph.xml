<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/Login">

    <fragment
        android:id="@+id/Search"
        android:name="com.example.meetmelive.Search"
        android:label="Search"
        tools:layout="@layout/fragment_search" />
    <fragment
        android:id="@+id/Nearby"
        android:name="com.example.meetmelive.Nearby"
        android:label="fragment_nearby"
        tools:layout="@layout/fragment_nearby" />
    <fragment
        android:id="@+id/Login"
        android:name="com.example.meetmelive.Login"
        android:label="Login"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/Register" />
    </fragment>
    <fragment
        android:id="@+id/Register"
        android:name="com.example.meetmelive.Register"
        android:label="Register"
        tools:layout="@layout/activity_register" />
    <fragment
        android:id="@+id/Profile"
        android:name="com.example.meetmelive.Profile"
        android:label="Profile"
        tools:layout="@layout/fragment_profile">
        <action
            android:id="@+id/action_Profile_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/matchProfileFragment"
        android:name="com.example.meetmelive.MatchProfile"
        android:label="Match Profile"
        tools:layout="@layout/fragment_match_profile" />
    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.example.meetmelive.EditProfile"
        android:label="Edit Profile"
        tools:layout="@layout/fragment_edit_profile" />
    <fragment
        android:id="@+id/deleteProfileFragment"
        android:name="com.example.meetmelive.DeleteProfile"
        android:label="fragment_delete_profile"
        tools:layout="@layout/fragment_delete_profile" />
</navigation>